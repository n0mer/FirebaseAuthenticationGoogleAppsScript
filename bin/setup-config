#!/usr/bin/env node

console.log(process.pwd)

const fs = require('fs');

const configPath = `${__dirname}/../.clasp.json`;
const claspJson = require(configPath);
const claspJsonOverride = require(`${configPath}.override.json`);

const merged = Object.assign(claspJson, claspJsonOverride);

fs.writeFileSync(configPath, JSON.stringify(merged));
